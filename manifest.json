{
  "manifest_version": 3,
  "name": "cfast",
  "version": "1.4",
  "description": "Speedup codeforces submission viewer",
  "host_permissions": ["https://codeforces.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["page.js"],
      "matches": ["https://codeforces.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://codeforces.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
